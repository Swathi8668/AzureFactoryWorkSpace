{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "azurefactoryworkspace"
		},
		"datalakestorage03_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'datalakestorage03'"
		},
		"datasynapseworkspace_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'datasynapseworkspace'"
		},
		"datalakestorage03_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakestorage03.dfs.core.windows.net/"
		},
		"datasynapseworkspace_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "datasynapseworkspace.sql.azuresynapse.net"
		},
		"datasynapseworkspace_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "datapool"
		},
		"datasynapseworkspace_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladminuser"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "azurefactoryworkspace_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"datalakestorage03_accountKey": {
						"value": "[parameters('datalakestorage03_accountKey')]"
					},
					"datasynapseworkspace_password": {
						"value": "[parameters('datasynapseworkspace_password')]"
					},
					"datalakestorage03_properties_typeProperties_url": {
						"value": "[parameters('datalakestorage03_properties_typeProperties_url')]"
					},
					"datasynapseworkspace_properties_typeProperties_server": {
						"value": "[parameters('datasynapseworkspace_properties_typeProperties_server')]"
					},
					"datasynapseworkspace_properties_typeProperties_database": {
						"value": "[parameters('datasynapseworkspace_properties_typeProperties_database')]"
					},
					"datasynapseworkspace_properties_typeProperties_userName": {
						"value": "[parameters('datasynapseworkspace_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}